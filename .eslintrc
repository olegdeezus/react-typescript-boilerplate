{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "semi": [1, "never"],
    "quotes": [1, "single"],
    "comma-dangle": [1, "always-multiline"],
    "no-cond-assign": [1, "always"],
    "no-console": [1, {"allow": ["warn", "error"]}],
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 1,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unexpected-multiline": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "use-isnan": 1,
    "valid-jsdoc": 1,
    "valid-typeof": 2,
    "accessor-pairs": [2, { "getWithoutSet": true }],
    "array-callback-return": 1,
    "block-scoped-var": 2,
    "consistent-return": 2,
    "curly": [2, "multi-line"],
    "default-case": 2,
    "eqeqeq": [2, "smart"],
    "guard-for-in": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-div-regex": 2,
    "no-else-return": 2,
    "no-empty-pattern": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": 2,
    "no-implicit-globals": 2,
    "no-implied-eval": 2,
    "no-invalid-this": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-magic-numbers": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unmodified-loop-condition": 2,
    "no-unused-labels": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-useless-escape": 2,
    "no-void": 2,
    "no-with": 2,
    "radix": 1,
    "vars-on-top": 1,
    "wrap-iife": [2, "inside"],
    "yoda": [2, "never", { "exceptRange": true }],
    "arrow-parens": [2, "always"],
    "arrow-body-style": [2, "as-needed"],
    "arrow-spacing": 1,
    "constructor-super": 2,
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-duplicate-imports": 2,
    "no-new-symbol": 2,
    "no-this-before-super": 2,
    "no-useless-computed-key": 2,
    "no-useless-constructor": 2,
    "no-var": 2,
    "object-shorthand": 1,
    "prefer-arrow-callback": 1,
    "prefer-const": 2,
    "prefer-rest-params": 1,
    "prefer-spread": 2,
    "prefer-template": 1,
    "require-yield": 2,
    "template-curly-spacing": [2, "never"],
    "block-spacing": 2,
    "brace-style": [2, "stroustrup"],
    "array-bracket-spacing": 1,
    "camelcase": 1,
    "comma-spacing": 1,
    "comma-style": 1,
    "computed-property-spacing": 1,
    "consistent-this": [2, "that"],
    "eol-last": 2,
    "func-names": 2,
    "func-style": [2, "expression"],
    "id-blacklist": [2, "data", "err", "e", "cb", "callback"],
    "id-length": 2,
    "indent": [2, "tab", {"SwitchCase": 1}],
    "jsx-quotes": [2, "prefer-double"],
    "key-spacing": 2,
    "linebreak-style": [2, "unix"],
    "lines-around-comment": [2, { "beforeBlockComment": true }],
    "max-depth": [2, 4],
    "max-len": [2, 80],
    "max-nested-callbacks": [2, 4],
    "max-params": [2, 3],
    "max-statements-per-line": [2, { "max": 2 }],
    "new-cap": 2,
    "new-parens": 2,
    "newline-after-var": 2,
    "newline-before-return": 2,
    "newline-per-chained-call": [2, { "ignoreChainWithDepth": 2 }],
    "no-array-constructor": 2,
    "no-bitwise": 2,
    "no-continue": 2,
    "no-inline-comments": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": [2, {"max": 2}],
    "no-negated-condition": 2,
    "no-nested-ternary": 2,
    "no-new-object": 2,
    "no-plusplus": [2, {"allowForLoopAfterthoughts": true }],
    "no-spaced-func": 2,
    "no-trailing-spaces": 2,
    "no-unneeded-ternary": 2,
    "no-whitespace-before-property": 2,
    "object-curly-spacing": [2, "always"],
    "one-var": [2, "never"],
    "operator-assignment": [2, "always"],
    "operator-linebreak": [2, "before"],
    "quote-props": [2, "as-needed"],
    "space-before-function-paren": 2,
    "space-infix-ops": 2,
    "spaced-comment": 2
  }
}
